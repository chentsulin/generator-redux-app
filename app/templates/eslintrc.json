{
  "parser": "babel-eslint",
  "extends": "airbnb",
  "env": {
    "browser": true,
    "node": true,
    "mocha": true
  },
  "rules": {
    "import/no-extraneous-dependencies": 0,
    "react/jsx-filename-extension": [2, { "extensions": [".js"] }]
  },
  "plugins": [
    "import",
    "react"
  ]
}
