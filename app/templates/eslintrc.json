{
  "parser": "babel-eslint",
  "extends": "airbnb",
  "env": {
    "browser": true,
    "node": true,
    "mocha": true
  },
  "rules": {
    "max-len": [2, 79, {
      "ignorePattern": " // eslint-disable-line "
    }],
    "no-underscore-dangle": 0,
    "import/no-extraneous-dependencies": 0,
    "react/jsx-filename-extension": [2, {
      "extensions": [".js"]
    }]
  },
  "plugins": [
    "import",
    "react"
  ]
}
