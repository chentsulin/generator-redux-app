{
  "name": "<%= moduleName %>",
  "version": "0.1.0",
  "description": "My module",
  "main": "lib/index.js",
  "license": "MIT",
  "repository": "<%= githubUsername %>/<%= moduleName %>",
  "scripts": {
    "start": "node server.js",
    "clean": "rimraf lib dist coverage",
    "lint": "eslint src test",
    "test": "NODE_ENV=test mocha --compilers js:babel-core/register --recursive --require ./test/setup.js",
    "test:watch": "npm test -- --watch",
    "test:cov": "NODE_ENV=test babel-node node_modules/.bin/isparta cover node_modules/.bin/_mocha -- --recursive --require ./test/setup.js",
    "check": "npm run lint && npm run test <% if (flow) { %> && flow check <% } %>",
    "build": "NODE_ENV=production webpack src/index.js --config webpack.config.production.js",
    "preversion": "npm run clean && npm run check",
    "version": "npm run build",
    "postversion": "git push && git push --tags && npm run clean",
    "prepublish": "npm run clean && npm run build"
  },
  "author": {
    "name": "<%= name %>",
    "email": "<%= email %>",
    "url": "<%= humanizedWebsite %>"
  },
  "engines": {
    "node": ">=0.10.0"
  },
  "keywords": [
    "redux",
    "<%= moduleName %>"
  ],
  "devDependencies": {
    "babel": "^6.3.26",
    "babel-cli": "^6.3.17",
    "babel-core": "^6.3.26",
    "babel-eslint": "^5.0.0-beta6",
    "babel-loader": "^6.2.0",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-react": "^6.3.13",
    "babel-preset-react-hmre": "^1.0.1",
    "babel-preset-stage-0": "^6.3.13",
    "chai": "^3.3.0",
    "eslint": "^1.6.0",
    "eslint-config-airbnb": "^2.1.1",
    "eslint-plugin-react": "^3.13.1",
    "express": "^4.13.3",
    "isparta": "^4.0.0",
    "jsdom": "^7.2.2",
    "mocha": "^2.2.5",
    "react-addons-test-utils": "^0.14.0",
    "redux-devtools": "^3.0.1",
    "redux-devtools-log-monitor": "^1.0.1",
    "rimraf": "^2.5.0",
    "sinon": "^1.17.1",
    "webpack": "^1.12.2",
    "webpack-dev-middleware": "^1.2.0",
    "webpack-hot-middleware": "^2.4.1"
  },
  "dependencies": {
    "history": "^1.17.0",
    "react": "^0.14.0",
    "react-dom": "^0.14.0",
    "react-redux": "^4.0.5",
    "react-router": "^1.0.3",
    "redux": "^3.0.2",
    "redux-logger": "^2.3.1",
    "redux-promise": "^0.5.0",
    "redux-thunk": "^1.0.0"
  }
}
