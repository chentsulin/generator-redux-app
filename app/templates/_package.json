{
  "name": "<%= moduleName %>",
  "version": "0.1.0",
  "description": "My module",
  "main": "lib/index.js",
  "license": "MIT",
  "repository": "<%= githubUsername %>/<%= moduleName %>",
  "scripts": {
    "start": "node server.js",
    "clean": "rimraf lib dist coverage",
    "lint": "eslint src test",
    "test": "cross-env NODE_ENV=test mocha --compilers js:babel-core/register --recursive --require ./test/setup.js",
    "test:watch": "npm test -- --watch",
    "test:cov": "cross-env NODE_ENV=test babel-node node_modules/.bin/isparta cover node_modules/.bin/_mocha -- --recursive --require ./test/setup.js",
    "check": "npm run lint && npm run test <% if (flow) { %> && flow check <% } %>",
    "build": "cross-env NODE_ENV=production webpack src/index.js --config webpack.config.production.js",
    "preversion": "npm run clean && npm run check",
    "version": "npm run build",
    "postversion": "git push && git push --tags && npm run clean",
    "prepublish": "npm run clean && npm run build"
  },
  "author": {
    "name": "<%= name %>",
    "email": "<%= email %>",
    "url": "<%= humanizedWebsite %>"
  },
  "engines": {
    "node": ">=0.10.0"
  },
  "keywords": [
    "redux",
    "<%= moduleName %>"
  ],
  "devDependencies": {
    "babel": "^6.5.2",
    "babel-cli": "^6.5.1",
    "babel-core": "^6.5.2",
    "babel-eslint": "^5.0.0-beta10",
    "babel-loader": "^6.2.2",
    "babel-preset-es2015": "^6.5.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-react-hmre": "^1.1.0",
    "babel-preset-stage-0": "^6.5.0",
    "chai": "^3.5.0",
    "cross-env": "^1.0.7",
    "eslint": "^1.10.3",
    "eslint-config-airbnb": "^5.0.1",
    "eslint-plugin-react": "^3.16.1",
    "express": "^4.13.4",
    "isparta": "^4.0.0",
    "jsdom": "^7.2.2",
    "mocha": "^2.4.5",
    "react-addons-test-utils": "^0.14.7",
    "redux-devtools": "^3.1.1",
    "redux-devtools-log-monitor": "^1.0.4",
    "rimraf": "^2.5.2",
    "sinon": "^1.17.3",
    "webpack": "^1.12.13",
    "webpack-dev-middleware": "^1.5.1",
    "webpack-hot-middleware": "^2.7.1"
  },
  "dependencies": {
    "react": "^0.14.7",
    "react-dom": "^0.14.7",
    "react-redux": "^4.4.0",
    "react-router": "^2.0.0",
    "react-router-redux": "^3.0.0",
    "redux": "^3.3.1",
    "redux-logger": "^2.5.2",
    "redux-promise": "^0.5.1",
    "redux-thunk": "^1.0.3"
  }
}
