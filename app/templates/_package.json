{
  "name": "<%= moduleName %>",
  "version": "0.1.0",
  "description": "My module",
  "main": "lib/index.js",
  "license": "MIT",
  "repository": "<%= githubUsername %>/<%= moduleName %>",
  "scripts": {
    "start": "node server.js",
    "clean": "rimraf lib dist coverage",
    "lint": "eslint src test",
    "test": "NODE_ENV=test mocha --compilers js:babel/register --recursive --require ./test/setup.js",
    "test:watch": "npm test -- --watch",
    "test:cov": "NODE_ENV=test babel-node $(npm bin)/isparta cover node_modules/mocha/bin/_mocha -- --recursive",
    "check": "npm run lint && npm run test <% if (flow) { %> && flow check <% } %>",
    "build": "NODE_ENV=production webpack src/index.js --config webpack.config.production.js",
    "preversion": "npm run clean && npm run check",
    "version": "npm run build",
    "postversion": "git push && git push --tags && npm run clean",
    "prepublish": "npm run clean && npm run build"
  },
  "author": {
    "name": "<%= name %>",
    "email": "<%= email %>",
    "url": "<%= humanizedWebsite %>"
  },
  "engines": {
    "node": ">=0.10.0"
  },
  "keywords": [
    "redux",
    "<%= moduleName %>"
  ],
  "devDependencies": {
    "babel": "^5.8.23",
    "babel-core": "^5.8.25",
    "babel-eslint": "^4.1.3",
    "babel-loader": "^5.3.2",
    "babel-plugin-react-transform": "^1.1.1",
    "chai": "^3.3.0",
    "eslint": "^1.6.0",
    "eslint-config-airbnb": "^0.1.0",
    "eslint-plugin-react": "^3.5.1",
    "express": "^4.13.3",
    "isparta": "3.0.2",
    "jsdom": "^6.5.1",
    "mocha": "^2.2.5",
    "react-addons-test-utils": "^0.14.0",
    "react-transform-catch-errors": "^1.0.0",
    "react-transform-hmr": "^1.0.0",
    "redbox-react": "^1.1.1",
    "redux-devtools": "^3.0.0",
    "redux-devtools-log-monitor": "^1.0.1",
    "rimraf": "^2.4.3",
    "sinon": "^1.17.1",
    "webpack": "^1.12.2",
    "webpack-dev-middleware": "^1.2.0",
    "webpack-hot-middleware": "^2.4.1"
  },
  "dependencies": {
    "history": "~1.13.0",
    "react": "^0.14.0",
    "react-dom": "^0.14.0",
    "react-redux": "^4.0.0",
    "react-router": "^1.0.0",
    "redux": "^3.0.2",
    "redux-logger": "^2.0.3",
    "redux-promise": "^0.5.0",
    "redux-thunk": "^1.0.0"
  }
}
