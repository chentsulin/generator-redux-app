{
  "name": "<%= moduleName %>",
  "version": "0.1.0",
  "description": "My module",
  "main": "lib/index.js",
  "license": "MIT",
  "repository": "<%= githubUsername %>/<%= moduleName %>",
  "scripts": {
    "start": "node server.js",
    "clean": "rimraf lib dist coverage",
    "lint": "eslint src test",
    "testonly": "cross-env NODE_ENV=test mocha --compilers js:babel-register --require ./scripts/mocha-setup.js src/**/__tests__/**/*.spec.js",
    "testonly:watch": "npm run testonly -- --watch",
    "test": "npm run lint && npm run testonly",
    "test:cov": "nyc npm test",
    "check": "npm run lint && npm run test <% if (flow) { %> && flow check <% } %>",
    "build": "cross-env NODE_ENV=production webpack src/index.js --config webpack.config.production.js",
    "preversion": "npm run clean && npm run check",
    "version": "npm run build",
    "postversion": "git push && git push --tags && npm run clean",
    "prepublish": "npm run clean && npm run build",
    "postinstall": "node node_modules/fbjs-scripts/node/check-dev-engines.js package.json"
  },
  "author": {
    "name": "<%= name %>",
    "email": "<%= email %>",
    "url": "<%= humanizedWebsite %>"
  },
  "devEngines": {
    "node": ">=4"
  },
  "keywords": [
    "redux",
    "<%= moduleName %>"
  ],
  "devDependencies": {
    "babel-cli": "^6.11.4",
    "babel-eslint": "^6.1.2",
    "babel-loader": "^6.2.4",
    "babel-preset-es2015": "^6.13.2",
    "babel-preset-react": "^6.11.1",
    "babel-preset-react-hmre": "^1.1.1",
    "babel-preset-stage-0": "^6.5.0",
    "babel-register": "^6.11.6",
    "chai": "^3.5.0",
    "chai-enzyme": "^0.5.0",
    "cross-env": "^2.0.0",
    "enzyme": "^2.4.1",
    "eslint": "^3.2.2",
    "eslint-config-airbnb": "^10.0.0",
    "eslint-plugin-import": "^1.12.0",
    "eslint-plugin-jsx-a11y": "^2.0.1",
    "eslint-plugin-react": "^6.0.0",
    "express": "^4.14.0",
    "fbjs-scripts": "^0.7.1",
    "jsdom": "^9.4.1",
    "mocha": "^3.0.1",
    "nyc": "^7.1.0",
    "react-addons-test-utils": "^15.3.0",
    "redux-devtools": "^3.3.1",
    "redux-devtools-log-monitor": "^1.0.11",
    "rimraf": "^2.5.4",
    "sinon": "^1.17.5",
    "sinon-chai": "^2.8.0",
    "webpack": "^1.13.1",
    "webpack-dev-middleware": "^1.6.1",
    "webpack-hot-middleware": "^2.12.2"
  },
  "dependencies": {
    "react": "^15.3.0",
    "react-dom": "^15.3.0",
    "react-redux": "^4.4.5",
    "react-router": "^2.6.1",
    "react-router-redux": "^4.0.5",
    "redux": "^3.5.2",
    "redux-logger": "^2.6.1",
    "redux-thunk": "^2.1.0"
  }
}
