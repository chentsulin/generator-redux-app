{
  "name": "<%= moduleName %>",
  "version": "0.1.0",
  "description": "My module",
  "main": "lib/index.js",
  "license": "MIT",
  "repository": "<%= githubUsername %>/<%= moduleName %>",
  "scripts": {
    "start": "webpack-dashboard -- node server.js",
    "clean": "rimraf lib dist coverage",
    "lint": "eslint src scripts *.js",
    "testonly": "cross-env NODE_ENV=test jest",
    "testonly:watch": "npm run testonly -- --watch",
    "test": "npm run lint && npm run testonly",
    "test:cov": "npm run lint && npm run testonly -- --coverage",
    "check": "npm run lint && npm run test <% if (flow) { %> && flow check <% } %>",
    "build": "npm run clean && cross-env NODE_ENV=production webpack src/index.js --config webpack.config.prod.js",
    "preversion": "npm run clean && npm run check",
    "version": "npm run build",
    "postversion": "git push && git push --tags && npm run clean",
    "prepublish": "npm run clean && npm run build",
    "postinstall": "node node_modules/fbjs-scripts/node/check-dev-engines.js package.json"
  },
  "author": {
    "name": "<%= name %>",
    "email": "<%= email %>",
    "url": "<%= humanizedWebsite %>"
  },
  "devEngines": {
    "node": ">=4"
  },
  "keywords": [
    "redux",
    "<%= moduleName %>"
  ],
  "devDependencies": {
    "autoprefixer": "^6.5.3",
    "babel-cli": "^6.18.0",
    "babel-eslint": "^7.1.1",
    "babel-jest": "^17.0.2",
    "babel-loader": "^6.2.8",
    "babel-plugin-dev-expression": "^0.2.1",
    "babel-plugin-webpack-loaders": "^0.8.0",
    "babel-preset-latest": "^6.16.0",
    "babel-preset-react": "^6.16.0",
    "babel-preset-react-optimize": "^1.0.1",
    "babel-preset-stage-2": "^6.18.0",
    "babel-register": "^6.18.0",
    "cross-env": "^3.1.3",
    "css-loader": "^0.26.1",
    "enzyme": "^2.6.0",
    "eslint": "^3.11.1",
    "eslint-config-airbnb": "^13.0.0",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^2.2.3",
    "eslint-plugin-react": "^6.8.0",
    "express": "^4.14.0",
    "extract-text-webpack-plugin": "^1.0.1",
    "fbjs-scripts": "^0.7.1",
    "html-webpack-plugin": "^2.24.1",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^17.0.3",
    "json-loader": "^0.5.4",
    "postcss-loader": "^1.2.0",
    "postcss-nested": "^1.0.0",
    "postcss-simple-vars": "^3.0.0",
    "react-addons-test-utils": "^15.4.1",
    "react-hot-loader": "^3.0.0-beta.6",
    "redux-logger": "^2.7.4",
    "remote-redux-devtools": "^0.5.5",
    "rimraf": "^2.5.4",
    "style-loader": "^0.13.1",
    "webpack": "^1.13.3",
    "webpack-dashboard": "^0.2.0",
    "webpack-dev-middleware": "^1.8.4",
    "webpack-hot-middleware": "^2.13.2"
  },
  "dependencies": {
   "react": "^15.4.1",
    "react-dom": "^15.4.1",
    "react-redux": "^4.4.6",
    "react-router": "^3.0.0",
    "react-router-redux": "^4.0.7",
    "redux": "^3.6.0",
    "redux-thunk": "^2.1.0"
  },
  "jest": {
    "moduleNameMapper": {
      "\\.css$": "<rootDir>/__mocks__/styleMock.js"
    }
  }
}
